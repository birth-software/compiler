const main = fn () s32 {
    const a = foo(5);
    if (a != 123) {
        return 1;
    }
    const b = foo(5);
    if (b != 123) {
        return 1;
    }
    return a - b;
}

const foo = fn (arg: s32) s32 {
    if (arg < 0) {
        return 12312;
    } else if (arg > 0) {
        return 123;
    } else {
        return 0;
    }
}
