const main = fn () s32 {
    const a = foo(5142);
    if (a != 2501) {
        return 1;
    }
    const b = foo(5142);
    if (b != 2501) {
        return 1;
    }
    return #cast(a - b);
}

const foo = fn (arg: u32) u32 {
    var i: u32 = 0;
    while (i < arg) {
        if (i > 2500) {
            return i;
        }

        i += 1;
    }

    return 321;
}
