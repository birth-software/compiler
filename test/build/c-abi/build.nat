const std = #import("std");
const Executable  = std.build.Executable;

const main = fn() *!void {
    const executable = Executable{
        .target = .{
            .cpu = .x86_64,
            .os = .linux,
            .abi = .gnu,
        },
        .main_source_path = "main.nat",
        .name = "c-abi",
        .c_source_files = .{ "foo.c" }.&,
    };

    try executable.compile();
}
